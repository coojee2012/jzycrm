<div class="container-fluid"><%if(msg!=null){%>
<div class="alert alert-block">
<button type="button" class="close" data-dismiss="alert">x</button>
<h4 class="alert-heading">注意!</h4>
<%= util.inspect(msg,false,null)%></div>

<%}%>

<div class="container-fluid">

<div class="row-fluid sortable">
<div class="box span12">
<div class="box-content">
<form class="form-horizontal" method="POST">
<div><a href="/ComplaintRecords/"
	class="btn btn-small btn-primary"> <i
	class="icon-chevron-left icon-white"></i>列表</a><!-- 
						  <a href="/ComplaintRecords/create" class="btn btn-small btn-primary">
						 <i class="icon-plus icon-white"></i>新增</a>  
						 --></div>
<fieldset><legend>投诉建议记录</legend>



<div class="control-group<%if(msg!=null &&
msg.CodeID!=null){%>error<%}%>"> <label class="control-label"
	for="CodeID">投诉建议类型 </label>
<div class="controls"><select id="CodeID" name="CodeID"
	data-rel="chosen">
</select></div>
</div>



<div class="control-group<%if(msg!=null &&
msg.Content!=null){%>error<%}%>"> <label class="control-label"
	for="Content">受理内容 </label>
<div class="controls"><textarea class="autogrow" id="Content"
	name="Content"><% if(inst!=null){%><%= inst.Content%><%}%></textarea>
<span class="help-inline"><%if(msg!=null &&
msg.Content!=null){%><%= msg.Content%><%}%></span></div>
</div>



<div class="control-group<%if(msg!=null &&
msg.answerContent!=null){%>error<%}%>"> <label class="control-label"
	for="answerContent">处理结果 </label>
<div class="controls"><textarea class="autogrow"
	id="answerContent" name="answerContent"><% if(inst!=null){%><%= inst.answerContent%><%}%></textarea>
<span class="help-inline"><%if(msg!=null &&
msg.answerContent!=null){%><%= msg.answerContent%><%}%></span></div>
</div>





<div class="control-group<%if(msg!=null &&
msg.answerStatus!=null){%>error<%}%>"> <label class="control-label"
	for="answerStatus">处理状态 </label>
<div class="controls"><label class="radio"> <input
	type="radio" id="answerStatus_0" name="answerStatus" value="0"<%
if(inst!=null && inst.answerStatus=='0' ){%> checked='checked' <% } else
if(inst==null){%> selected=&quot;selected&quot; <% }else{ %> <% }%> />
未处理 </label> <label class="radio"> <input type="radio"
	id="answerStatus_1" name="answerStatus" value="1"<%
if(inst!=null && inst.answerStatus=='1' ){%> checked='checked' <% } else
if(inst==null){%> <% }else{ %> <% }%> /> 处理中 </label> <label class="radio">
<input type="radio" id="answerStatus_2" name="answerStatus" value="2"<%
if(inst!=null && inst.answerStatus=='2' ){%> checked='checked' <% } else
if(inst==null){%> <% }else{ %> <% }%> /> 已完成 </label></div>
</div>



<input type="hidden" id="id" name="id" value="<%
if(inst!=null){%><%= inst.id %><% }%>" />
<div class="form-actions">
<button type="submit" class="btn btn-primary">保存</button>
<button type="reset" class="btn">取消</button>
</div>
</fieldset>
</form>

</div>
</div>
<!--/span--></div>
<!--/row--></div>
<!-- div containt --> <script type="text/javascript"><!--

$(function(){
	 $.ajax(
	            {
	                url: '/selectdb/',
	                dataType: 'json',
	                type: 'POST',
	                async: false,
	                data:{tbname:'crm/ComplaintCode',key:'id',value:'CodeName'},
	                success: function (data, textStatus) {
	                	if(data.success)
                        {
                        	for (var i = 0; i < data.data.length; i++) {
                        		<%if(inst!=null){ %>
	                        if(data.data[i].id==<%= inst.CodeID%>)
	                        {
	                        	$("<option value='" + data.data[i].id + "' selected='selected' >" + data.data[i].CodeName + "</option>").appendTo("#CodeID");	
		                        }
	                        <%}else{%>
	                        if(false){}
	                        <%}%>
	                        else{
                        	$("<option value='" + data.data[i].id + "'  >" + data.data[i].CodeName + "</option>").appendTo("#CodeID");	
	                        }
                        	}
	                        }
	                        else{
	                        	alert(data.msg);
		                        }
	                },
	                error: function (XMLHttpRequest, textStatus, errorThrown) {
	                    alert('服务器访问发生异常！' + textStatus);
	                    return;
	                }

	            }
	            );	
	
});



