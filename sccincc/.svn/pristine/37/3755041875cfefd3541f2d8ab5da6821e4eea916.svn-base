[trunkgroups]
[channels]
context=from-pstn
;signalling=fxs_ks
;rxwink=300 ; Atlas seems to use long (250ms) winks
usecallerid=yes
hidecallerid=no
callwaiting=yes
usecallingpres=yes
callwaitingcallerid=yes
threewaycalling=yes
transfer=yes
canpark=yes
cancallforward=yes
callreturn=yes
echocancel=no
echocancelwhenbridged=no
;faxdetect=incoming
echotraining=undefined
rxgain=0.0
txgain=0.0
callgroup=1
pickupgroup=1
cidsignalling=bell
cidstart=ring
;Uncomment these lines if you have problems with the disconection of your analog lines
busydetect=yes
busycount=5
;busypattern=
callprogress=no
pulsedial=no
; both incoming outgoing no
faxdetect=no
answeronpolarityswitch=no
hanguponpolarityswitch=no


;immediate=no

#include dahdi_additional.conf
#include dahdi-channels.conf

