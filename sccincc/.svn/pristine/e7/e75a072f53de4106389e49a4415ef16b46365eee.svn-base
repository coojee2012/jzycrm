<div class="container-fluid">
<div class="container-fluid">
<div class="row-fluid sortable">
<div class="box span12">
<div class="box-content">
<form class="form-horizontal" method="POST">
<div><a href="/ConsultRecords/" class="btn btn-small btn-primary">
<i class="icon-chevron-left icon-white"></i>列表</a> <!--
						  <a href="/ConsultRecords/create" class="btn btn-small btn-primary">
						  <i class="icon-plus icon-white"></i>新增</a> 
						  --> <a href="/ConsultRecords/edit?id=<%= inst['id']%>"
class="btn btn-small btn-primary"> <i class="icon-edit icon-white"></i>修改</a>
</div>
<fieldset><legend>咨询记录</legend>

<div class="control-group"><label class="control-label"
	for="callUnitID">咨询编号 </label>
<div class="controls"></div>
</div>

<div class="control-group"><label class="control-label" for="ctID">咨询类型
</label>
<div class="controls"><select readonly="readonly" id="ctID"
	name="ctID" data-rel="chosen">
</select></div>
</div>

<div class="control-group"><label class="control-label"
	for="ctContent">咨询内容 </label>
<div class="controls"><textarea class="autogrow"
	readonly="readonly" id="ctContent" name="ctContent"><%= inst['ctContent']%></textarea>
</div>
</div>


</div>

<div class="control-group"><label class="control-label"
	for="isNeedBack">是否回访 </label>
<div class="controls"><label class="radio"> <input
	type="radio" readonly="readonly" class="uneditable-input"
	id="isNeedBack_0" name="isNeedBack" value="0"<%if(inst.isNeedBack=='0'){%>checked="checked"<%}%>
/> 未处理 </label> <label class="radio"> <input type="radio"
	readonly="readonly" class="uneditable-input" id="isNeedBack_1"
	name="isNeedBack" value="1"<%if(inst.isNeedBack=='1'){%>checked="checked"<%}%>
/> 已处理 </label></div>
</div>

<div class="control-group"><label class="control-label"
	for="backStatus">回访状态 </label>
<div class="controls"><label class="radio"> <input
	type="radio" readonly="readonly" class="uneditable-input"
	id="backStatus_0" name="backStatus" value="0"<%if(inst.backStatus=='0'){%>checked="checked"<%}%>
/> 未处理 </label> <label class="radio"> <input type="radio"
	readonly="readonly" class="uneditable-input" id="backStatus_1"
	name="backStatus" value="1"<%if(inst.backStatus=='1'){%>checked="checked"<%}%>
/> 已处理 </label></div>
</div>

<div class="control-group"><label class="control-label"
	for="backType">回访方式 </label>
<div class="controls"><select readonly="readonly" id="backType"
	name="backType" data-rel="chosen">

	<option value="1"<%if(inst.backType=='1'){%>selected="selected"<%}%>
	> 电话</option>

	<option value="2"<%if(inst.backType=='2'){%>selected="selected"<%}%>
	> 短信</option>

	<option value="3"<%if(inst.backType=='3'){%>selected="selected"<%}%>
	> 其他</option>

</select></div>
</div>

<div class="control-group"><label class="control-label"
	for="ansContent">受理内容 </label>
<div class="controls"><textarea class="autogrow"
	readonly="readonly" id="ansContent" name="ansContent"><%= inst['ansContent']%></textarea>
















</div>
</div>

<div class="control-group"><label class="control-label"
	for="isOver">受理结果 </label>
<div class="controls"><label class="radio"> <input
	type="radio" readonly="readonly" class="uneditable-input" id="isOver_0"
	name="isOver" value="0"<%if(inst.isOver=='0'){%>checked="checked"<%}%>
/> 未处理 </label> <label class="radio"> <input type="radio"
	readonly="readonly" class="uneditable-input" id="isOver_1"
	name="isOver" value="1"<%if(inst.isOver=='1'){%>checked="checked"<%}%>
/> 已处理 </label></div>
</div>

<div class="control-group"><label class="control-label"
	for="backContent">回访内容 </label>
<div class="controls"><textarea class="autogrow"
	readonly="readonly" id="backContent" name="backContent"><%= inst['backContent']%></textarea>
















</div>
</div>

<div class="control-group"><label class="control-label"
	for="srvManID">服务座席 </label>
<div class="controls"><select readonly="readonly" id="srvManID"
	name="srvManID" data-rel="chosen">
</select></div>
</div>

<div class="control-group"><label class="control-label"
	for="backMan">回访人员 </label>
<div class="controls"><select readonly="readonly" id="backMan"
	name="backMan" data-rel="chosen">
</select></div>
</div>

<div class="control-group"><label class="control-label" for="cID">用户名称
</label>
<div class="controls"><select readonly="readonly" id="cID"
	name="cID" data-rel="chosen">
</select></div>
</div>

<div class="control-group"><label class="control-label"
	for="srvScode">用户评分 </label>
<div class="controls"><span class="input-xlarge uneditable-input"><%=
inst['srvScode']%></span></div>
</div>

<div class="control-group"><label class="control-label"
	for="rbackTime">回访时间 </label>
<div class="controls"><span class="input-xlarge uneditable-input"><%=
inst['rbackTime']%></span></div>
</div>

<div class="control-group"><label class="control-label"
	for="backTime">回访时间 </label>
<div class="controls"><span class="input-xlarge uneditable-input"><%=
inst['backTime']%></span></div>
</div>

<div class="control-group"><label class="control-label"
	for="ansTime">记录时间 </label>
<div class="controls"><span class="input-xlarge uneditable-input"><%=
inst['ansTime']%></span></div>
</div>



</fieldset>
</form>

</div>
</div>
<!--/span--></div>
<!--/row--></div>
<!-- div containt --> <script type="text/javascript">

$(function(){
	 $.ajax(
	            {
	                url: '/selectdb/',
	                dataType: 'json',
	                type: 'POST',
	                async: false,
	                data:{tbname:'crm/ConsultCode',key:'id',value:'ctName'},
	                success: function (data, textStatus) {
	                        if(data.success)
	                        {
	                        	for (var i = 0; i < data.data.length; i++) {
		                        if(data.data[i].id==<%= inst.id%>)
		                        {
		                        	$("<option value='" + data.data[i].id + "' selected='selected' >" + data.data[i].ctName + "</option>").appendTo("#ctID");	
			                        }
		                        else{
	                        	$("<option value='" + data.data[i].id + "'  >" + data.data[i].ctName + "</option>").appendTo("#ctID");	
		                        }
	                        	}
		                        }
	                        else{
	                        	alert(data.msg);
		                        }
	                },
	                error: function (XMLHttpRequest, textStatus, errorThrown) {
	                    alert('服务器访问发生异常！' + textStatus);
	                    return;
	                }

	            }
	            );	
	
});

